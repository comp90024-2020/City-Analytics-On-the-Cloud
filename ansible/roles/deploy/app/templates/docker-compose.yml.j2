version: '3'
services:
  app:
    build: ./
      ports:
        - 80:3000
    environment:
        - DB_USERNAME={{ couch.user }}
        - DB_PASSWORD={{ couch.password }}
    networks:
        - comp90024
    links:
        - "couchdb:couchdb"
networks:
  comp90024:
    external:
      name: comp90024_network
